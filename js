fetch('https://52.78.7.40:3000/communities/17', { method: "post"}).then( (res) => { res.json().then( json => { console.log(json); var div = document.createElement('div'); div.setAttribute('class','community_list'); for(i in json){  console.log(i); div.innerHTML += "<div class='tit'> title: " + json[i]["title"] + "</div>" + "<div> 작성자 이름: " + json[i]["writername"] + "</div>" +  "<div> 하트수: "+ json[i]["heart"] + "</div>" + div.innerHTML;  } return div;} ).then( (div) => { console.log(div) } ); } ).then( () => {  console.log("ok") });
