<form class="propose_form w-100 font-sm-10" action="/propose/<%= params[:id] %>/subscribe" accept-charset="UTF-8" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token, id: :form_token %>
  <input type="email" class="form-control" name="email" placeholder="내 이메일 주소">
  <input type="text" class="form-control" name="phone" placeholder="내 전화번호">
  <input type="submit">
</form>